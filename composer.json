{
    "name": "illuminatech/model-rules",
    "description": "Set of the validation rules to check if a model exists or is unique for Laravel",
    "keywords": ["laravel", "validation", "rule", "model", "eloquent", "exist", "unique"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/model-rules/issues",
        "wiki": "https://github.com/illuminatech/model-rules/wiki",
        "source": "https://github.com/illuminatech/model-rules"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/validation": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "illuminate/database": "*",
        "illuminate/config": "*",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.3 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\ModelRules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\ModelRules\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}